{
  "presets": ["next/babel"],
  "plugins": [
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false
      }
    ],
    [
      "module-resolver",
      {
        "alias": {
          "components": "./src/components",
          "tips": "./src/tips",
          "utils": "./src/utils.js"
        }
      }
    ]
  ]
}
